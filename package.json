{
  "name": "unlock_protocol",
  "license": "UNLICENSED",
  "scripts": {
    "precodegen" : "yarn generate-subgraph-yaml",
    "codegen": "graph codegen",
    "generate-subgraph-yaml": "node ./bin/subgraph_generator.js",
    "prebuild": "yarn generate-subgraph-yaml",
    "build": "graph build",
    "predeploy-local": "yarn generate-subgraph-yaml",
    "create-local": "graph create --node http://localhost:8020/ unlock-protocol/unlock",
    "remove-local": "graph remove --node http://localhost:8020/ unlock-protocol/unlock",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 unlock-protocol/unlock",
    "create-kovan-local": "graph create --node http://localhost:8020/ unlock-protocol/unlock-kovan",
    "remove-kovan-local": "graph remove --node http://localhost:8020/ unlock-protocol/unlock-kovan",
    "deploy-kovan-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 unlock-protocol/unlock-kovan",
    "deploy:kovan": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ unlock-protocol/unlock-kovan"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.15.0",
    "@graphprotocol/graph-ts": "0.15.0",
    "handlebars": "^4.7.3",
    "yargs": "^15.1.0"
  }
}
